{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "react/no-unescaped-entities": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "prefer-const": "warn",
    "no-empty": "warn",
    "no-dupe-keys": "error",
    "no-undef": "off"
  }
} 